{
  "name": "yabai",
  "version": "0.0.10",
  "repository": "git@github.com:mk2/yabai.git",
  "bin": {
    "yabai": "./yabai.js"
  },
  "main": "./lib/main.js",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prepare": "run-s clean:* prepare:* build:js",
    "prepare:data": "cp -R fixtures/data data/",
    "start": "BOOSTNOTE_DATA_DIRECTORY=data && node lib/main.js",
    "clean-start": "run-s prepare start",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check -- --watch",
    "clean:js": "rimraf lib dist",
    "clean:log": "rimraf yabai.log",
    "clean:data": "rimraf data",
    "build:js": "babel src --out-dir lib --extensions \".ts,.js\" --source-maps inline",
    "build:types": "tsc --emitDeclarationOnly",
    "bundle": "run-s build:* bundle:js",
    "bundle:js": "webpack",
    "test": "jest"
  },
  "dependencies": {
    "autobind-decorator": "^2.4.0",
    "blessed": "*",
    "cson-parser": "https://github.com/mk2/cson-parser.git#master",
    "lodash-es": "^4.17.15",
    "luxon": "^1.21.3",
    "marked": "^0.8.0",
    "marked-terminal": "^4.0.0",
    "mobx": "^5.15.1",
    "mobx-method-decorators": "^0.0.4",
    "mobx-utils": "^5.5.3",
    "node-pty": "^0.9.0",
    "pino": "^5.15.0",
    "term.js": "^0.0.7",
    "text-buffer": "^13.17.2",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-numeric-separator": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/plugin-proposal-private-methods": "^7.8.3",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-typescript": "^7.7.7",
    "@types/blessed": "*",
    "@types/cson": "^0.0.31",
    "@types/lodash-es": "^4.17.3",
    "@types/luxon": "^1.21.0",
    "@types/marked": "^0.7.2",
    "@types/marked-terminal": "^3.1.1",
    "@types/pino": "^5.15.0",
    "@types/text-buffer": "^13.0.4",
    "@types/uuid": "^3.4.6",
    "babel-plugin-module-resolver": "^4.0.0",
    "native-addon-loader": "^2.0.1",
    "null-loader": "^3.0.0",
    "rimraf": "^3.0.0",
    "terser-webpack-plugin": "^2.3.1",
    "type-fest": "^0.8.1",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10"
  }
}
